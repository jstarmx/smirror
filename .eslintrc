{
  "plugins": ["prettier"],
  "extends": ["eslint-config-airbnb-base", "prettier"],
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": 0,
    "import/no-named-as-default": 2,
    "import/no-named-as-default-member": 2,
    "linebreak-style": 0,
    "no-console": 1,
    "prettier/prettier": 2
  }
}
